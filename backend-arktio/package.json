{
  "name": "backend-arktio",
  "version": "1.0.0",
  "description": "Backend for the game server of Arktio",
  "repository": "git@git.unistra.fr:rnoel/projet-arktio.git",
  "author": "2B3",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node build/index.js",
    "build": "rimraf build && tsc",
    "build-start": "npm run build && npm run start",
    "clean": "rimraf build coverage",
    "dev": "tsc-watch --onSuccess \"node ./build/index.js\"",
    "test": "jest --reporters=default --detectOpenHandles",
    "migrate": "npx knex migrate:latest --knexfile ./src/bdd/knexfile.ts",
    "reset": "npx knex migrate:rollback --knexfile ./src/bdd/knexfile.ts",
    "seed": "npx knex seed:run --knexfile ./src/bdd/knexfile.ts"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express-session": "^1.17.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "knex": "^1.0.3",
    "mysql2": "^2.3.3",
    "objection": "^3.0.1",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "ts-node": "^10.6.0",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/socket.io": "^3.0.2",
    "eslint": "^8.8.0",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "tsc-watch": "^4.6.0"
  }
}
